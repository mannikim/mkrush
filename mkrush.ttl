@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .

<https://github.com/mannikim/mkrush> 
	a lv2:Plugin , lv2:FilterPlugin, doap:Project ;
	lv2:binary <mkrush.so> ;
	doap:name "mkrush" ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port [
		a lv2:AudioPort , lv2:InputPort ;
                lv2:index 0 ;
                lv2:symbol "in" ;
                lv2:name "In" ;
	] , [
		a lv2:AudioPort , lv2:OutputPort ;
		lv2:index 1 ;
                lv2:symbol "out" ;
                lv2:name "Out" ;
	] , [
		a lv2:InputPort , lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "scale" ;
                lv2:name "Scale" ;
		lv2:default 100.0 ;
                lv2:minimum 0.5 ;
                lv2:maximum 1000.0 ;
	] , [
		a lv2:InputPort , lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "hold" ;
                lv2:name "Hold" ;
		lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 500 ; 
		lv2:portProperty lv2:integer ;
	], [
		a lv2:InputPort , lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "enable" ;
		lv2:name "Enable Scaling" ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
		lv2:portProperty lv2:toggled ;
	] .
